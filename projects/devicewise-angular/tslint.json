{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ]
  },
  "compilerOptions": {
    "paths": {
      "ngx-cookie-service/*": [
        "./node_modules/ngx-cookie-service/*"
      ]
    }
  }
}
